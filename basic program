#include <stdio.h>

int main()
{
    float d,y,m,w;
    scanf ("%f",&d);
    y=d/365.0;
    m=d/30.0;
    w=d/7.0;
    printf("%f years %f months %f weeks",y,m,w);
    
    return 0;
}
