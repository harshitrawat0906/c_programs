

#include <stdio.h>

int main()
{
    int a, b ,sum;
    printf("enter no\n");
    scanf("%d%d",&a,&b);
        sum=a+b;
        printf("%d",sum);
    return 0;
}

________________________________________________________________________________________

#include <stdio.h>

int main()
{
    int a;
    double b;
    float c;
    char d;
    scanf("%c",&d);
    scanf("%d",&a);
    scanf("%lf",&b);
    scanf("%f",&c);
    printf("%c\n",d);
    printf("%d\n",a);
    printf("%lf\n",b);
    printf("%f\n",c);
    return 0;
}

